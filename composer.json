{
    "name": "caoym/phpboot",
    "description": "A Spring Boot like PHP framework.",
    "keywords": [
        "phpboot",
        "restful",
        "boot"
    ],
    "license": "MIT",
    "require": {
        "php":">=5.4",
        "monolog/monolog": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "authors": [
        {
            "name": "caoyangmin",
            "email": "caoyangmin@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpBoot\\": "src/",
            "PhpBoot\\Tests\\": "tests/"
        },
        "files": [
            "src/Utils/fail.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://composer.intra.ffan.com/repo/private/"
        },
        {
            "type": "composer",
            "url": "http://composer.intra.ffan.com/repo/packagist/"
        },
        {
            "packagist": false
        }
    ]
}
