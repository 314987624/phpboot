{
    "name": "caoym/phpboot",
    "description": "PHP RESTful API framework.",
    "keywords": [
        "phpboot",
        "restful",
        "boot"
    ],
    "license": "MIT",
    "require": {
        "php":">=5.5.9",
        "ext-apc": "*",
        "monolog/monolog": "~1.0",
        "symfony/http-foundation": "^3.3",
        "vlucas/valitron": "^1.4",
        "doctrine/cache": "^1.6",
        "phpdocumentor/reflection-docblock": "^3.1",
        "mtdowling/jmespath.php": "^2.4",
        "nikic/fast-route": "^1.2",
        "symfony/http-kernel": "^3.3",
        "php-di/php-di": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "authors": [
        {
            "name": "Yangmin Cao",
            "email": "caoyangmin@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpBoot\\": "src/",
            "PhpBoot\\Tests\\": "tests/"
        },
        "files": [
            "src/functions.php",
            "src/Annotation/Names.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
